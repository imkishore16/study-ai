{
    "version": 2,
    "builds": [
      {
        "src": "embedchain-backend/main.py",
        "use": "@vercel/python"
      },
      {
        "src": "next.config.js",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      {
        "src": "^/api/(.*)$",
        "dest": "embedchain-backend/main.py"
      },
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
  }
  